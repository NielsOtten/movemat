{
  "presets": ["kyt-react"],
  "env": {
    "development": {
      "plugins": [
        "transform-decorators-legacy",
        "transform-object-rest-spread",
        "syntax-object-rest-spread"
      ]
    },
    "production": {
      "plugins": [
        "transform-object-rest-spread",
        "syntax-object-rest-spread",
        "transform-react-remove-prop-types"
      ]
    }
  }
}
